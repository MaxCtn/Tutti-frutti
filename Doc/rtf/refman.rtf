00097             $origin = $request->query->get({\cf22 'origin'}, {\cf22 'search'});\par
00098         \} {\cf19 catch} (\\Exception $e) \{\par
00099             $this->addFlash({\cf22 'error'}, {\cf22 'Impossible de r\'E9cup\'E9rer les d\'E9tails de cet album.'});\par
00100             {\cf19 return} $this->redirectToRoute({\cf22 'album_search'});\par
00101         \}\par
00102 \par
00103         {\cf19 return} $this->render({\cf22 'album/details.html.twig'}, [\par
00104             {\cf22 'album'} => $albumDetails,\par
00105             {\cf22 'origin'} => $origin, {\cf20 // Passer l'origine \'E0 la vue}\par
00106         ]);\par
00107     \}\par
00108 \par
00118     {\cf17 public} {\cf17 function} addToFavorites({\cf18 int} $id): JsonResponse\par
00119     \{\par
00120         $user = $this->getUser();\par
00121 \par
00122         {\cf19 if} (!$user) \{\par
00123             {\cf19 return} {\cf17 new} JsonResponse([{\cf22 'error'} => {\cf22 'Utilisateur non authentifi\'E9'}], 401);\par
00124         \}\par
00125 \par
00126         {\cf19 try} \{\par
00127             $albumDetails = $this->discogsService->getAlbumDetails($id);\par
00128 \par
00129             {\cf19 if} (empty($albumDetails[{\cf22 'fruits'}])) \{\par
00130                 {\cf19 return} {\cf17 new} JsonResponse([{\cf22 'error'} => {\cf22 'Cet album ne contient aucun fruit pertinent.'}], 400);\par
00131             \}\par
00132 \par
00133             $existingAlbum = $this->getFavoriteAlbumByUserAndId($user, $albumDetails[{\cf22 'id'}]);\par
00134 \par
00135             {\cf19 if} ($existingAlbum) \{\par
00136                 {\cf19 return} {\cf17 new} JsonResponse([{\cf22 'error'} => {\cf22 'Cet album est d\'E9j\'E0 dans vos favoris'}], 409);\par
00137             \}\par
00138 \par
00139             $this->createFavoriteAlbum($user, $albumDetails);\par
00140 \par
00141             {\cf19 return} {\cf17 new} JsonResponse([{\cf22 'success'} => {\cf17 true}]);\par
00142         \} {\cf19 catch} (\\Exception $e) \{\par
00143             {\cf19 return} {\cf17 new} JsonResponse([{\cf22 'error'} => {\cf22 'Erreur lors de l\\'ajout de l\\'album'}], 500);\par
00144         \}\par
00145     \}\par
00146 \par
00156     {\cf17 public} {\cf17 function} removeFromFavorites({\cf18 int} $id): Response\par
00157     \{\par
