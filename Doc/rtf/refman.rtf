00165                 $this->entityManager->flush();\par
00166 \par
00167                 $this->addFlash({\cf22 'success'}, {\cf22 'L\\'album a \'E9t\'E9 retir\'E9 de vos favoris.'});\par
00168             \}\par
00169         \}\par
00170 \par
00171         {\cf19 return} $this->redirectToRoute({\cf22 'profile'});\par
00172     \}\par
00173 \par
00182     {\cf17 private} {\cf17 function} getFavoriteAlbumByUserAndId($user, {\cf18 int} $albumId): ?FavoriteAlbum\par
00183     \{\par
00184         {\cf19 return} $this->entityManager->getRepository(FavoriteAlbum::class)\par
00185             ->findOneBy([{\cf22 'user'} => $user, {\cf22 'albumId'} => $albumId]);\par
00186     \}\par
00187 \par
00194     {\cf17 private} {\cf17 function} createFavoriteAlbum($user, array $albumDetails): void\par
00195     \{\par
00196         $favoriteAlbum = {\cf17 new} FavoriteAlbum();\par
00197         $favoriteAlbum->setUser($user);\par
00198         $favoriteAlbum->setAlbumId($albumDetails[{\cf22 'id'}]);\par
00199         $favoriteAlbum->setTitle($albumDetails[{\cf22 'title'}]);\par
00200         $favoriteAlbum->setYear($albumDetails[{\cf22 'year'}] ?? {\cf17 null});\par
00201         $favoriteAlbum->setCoverImage($albumDetails[{\cf22 'coverImage'}] ?? {\cf22 '/images/placeholder.jpg'});\par
00202 \par
00203         {\cf20 // Associe les fruits pertinents \'E0 l'album favori}\par
00204         {\cf19 foreach} ($albumDetails[{\cf22 'fruits'}] as $fruitName) \{\par
00205             $fruit = $this->findOrCreateEntity(Fruit::class, [{\cf22 'name'} => $fruitName]);\par
00206             $favoriteAlbum->addFruit($fruit);\par
00207         \}\par
00208 \par
00209         $this->entityManager->persist($favoriteAlbum);\par
00210         $this->entityManager->flush();\par
00211     \}\par
00212 \par
00221     {\cf17 private} {\cf17 function} findOrCreateEntity({\cf18 string} $entityClass, array $criteria)\par
00222     \{\par
00223         $entity = $this->entityManager->getRepository($entityClass)->findOneBy($criteria);\par
00224 \par
00225         {\cf19 if} (!$entity) \{\par
00226             $entity = {\cf17 new} $entityClass();\par
00227             {\cf19 foreach} ($criteria as $property => $value) \{\par
00228                 $setter = {\cf22 'set'} . ucfirst($property);\par
00229                 {\cf19 if} (method_exists($entity, $setter)) \{\par
00230                     $entity->$setter($value);\par
00231                 \}\par
00232             \}\par
00233             $this->entityManager->persist($entity);\par
00234             $this->entityManager->flush();\par
00235         \}\par
00236 \par
00237         {\cf19 return} $entity;\par
00238     \}\par
00239 \}\par
00240 \par
}
